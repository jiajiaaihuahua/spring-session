<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">
    
<struts>
    <package name="default" extends="struts-default" namespace="/">
       <action name="saveUser" class="online.shixun.project.spring.controller.UserController" method="saveUser">
       		<result name="success">/WEB-INF/view/saveUser.jsp</result>
       </action>
       
       <action name="getUser" class="online.shixun.project.spring.controller.UserController" method="getUser">
       		<result name="success">/WEB-INF/view/showUser.jsp</result>
       </action>
    </package>
 
</struts>